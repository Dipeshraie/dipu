package allinone;

public class Product {
	private long id;
	private String name;
	private String manufacturer;
	private String description;
	private float price;
	public Product(long id, String name, String manufacturer, String description, float price) {
		super();
		this.id = id;
		this.name = name;
		this.manufacturer = manufacturer;
		this.description = description;
		this.price = price;
	}
	
	public Product(String name, String manufacturer, String description, float price) {
		super();
		this.name = name;
		this.manufacturer = manufacturer;
		this.description = description;
		this.price = price;
	}
	
	
	

}
