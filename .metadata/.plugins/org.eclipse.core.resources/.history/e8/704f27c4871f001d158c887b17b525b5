package selfproect;

	class Student{
		String name;
		int age;
		int phonenumber;
		public void printinfo() {
			System.out.println(this.name);
			System.out.println(this.age);
			System.out.println(this.phonenumber);
		}
		Student(Student s2){
			this.name=s2.name;
			this.age=s2.age;
		}
		Student(){}
		
		
	}

public class oops {
	public static void main(String[] args) {
		Student s1=new Student();
		s1.name="dipesh";
		s1.age=21;
		Student s2=new Student(s1);
		s2.printinfo();
		
		
	}
	
}
